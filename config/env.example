SNOWFLAKE_ACCOUNT=
SNOWFLAKE_ROLE=
SNOWFLAKE_WAREHOUSE=
SNOWFLAKE_DATABASE=
SNOWFLAKE_SCHEMA=
SNOWFLAKE_USER=
SNOWFLAKE_PASSWORD=
SNOWFLAKE_OCSP_FAIL_OPEN=false
SNOWFLAKE_DISABLE_OCSP_CHECKS=false

# Local Cortex Analyst REST (PAT only)
SNOWFLAKE_TOKEN=

# Optional (LLM bump chart summaries)
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4o-mini
SNOWFLAKE_ACCOUNT=your_account
SNOWFLAKE_USER=your_username
SNOWFLAKE_PASSWORD=your_password
SNOWFLAKE_ROLE=your_role
SNOWFLAKE_WAREHOUSE=your_warehouse
SNOWFLAKE_DATABASE=your_database
SNOWFLAKE_SCHEMA=your_schema
SNOWFLAKE_OCSP_FAIL_OPEN=false
SNOWFLAKE_DISABLE_OCSP_CHECKS=false

# FEMA dataset table
FEMA_TABLE=your_table
# Optional override if you want to set a fully qualified name directly
# FEMA_TABLE_FQN=database.schema.table
# Unified joined view (recommended)
# FEMA_JOINED_VIEW_FQN=your_database.your_schema.FEMA_DISASTER_UNIFIED_TABLE
# Relation type (TABLE or VIEW)
FEMA_RELATION_TYPE=VIEW

# Column mappings (override if your schema uses different names)
# Defaults for the unified FEMA view
FEMA_COL_STATE=STATE_GEO_ID
FEMA_COL_INCIDENT_TYPE=INCIDENT_TYPE
FEMA_COL_DECLARATION_DATE=DISASTER_DECLARATION_DATE
FEMA_COL_LATITUDE=REGION_LATITUDE
FEMA_COL_LONGITUDE=REGION_LONGITUDE
FEMA_COL_DISASTER_ID=DISASTER_ID

# Query limits and Cortex model
FEMA_QUERY_LIMIT=5000
SNOWFLAKE_CORTEX_MODEL=snowflake-arctic
